     Period , Phase , Indicator );
     CellPutN( (((-1) * vAmount )+ Elim_Mgmt_interco) , zCube , Activity , Currency , 'Eliminations_Mgmt_interco' , Legal_Organization , 
     Management_Organization , Period , Phase , Indicator );
ENDIF;


################################################################################
#                                                               INPUT SECTION : Cube => ST_Partner_YTD
################################################################################

Cumul = CellGetN ( zCubePar , 'NO_APP' ,  Activity , Currency ,Gaap, Legal_Organization , Management_Organization , Partner , 
Period , Phase , Indicator );
CellPutN(vAmount + Cumul , zCubePar , 'NO_APP' ,  Activity , Currency , Gaap,Legal_Organization , Management_Organization , 
Partner , Period , Phase , Indicator );

#--------------------------------------------------- Eliminations Management Interco
#-- If Partner ≠ SU9999, YS7998 or other VE entities then
IF( Partner @<> 'SU9999' & Partner @<> 'YS7998' & Partner @<> 'Other_VE_entities' ); 
     Elim_Mgmt_interco = CellGetN ( zCubePar , 'NO_APP' ,  Activity , Currency , 'Eliminations_Mgmt_interco' , Legal_Organization , Management_Organiz
ation , Partner , 
     Period , Phase , Indicator );
     CellPutN( (((-1) * vAmount )+ Elim_Mgmt_interco) , zCubePar , 'NO_APP' ,  Activity , Currency , 'Eliminations_Mgmt_interco' , Legal_Organization 
, Management_Organization , Partner , 
     Period , Phase , Indicator );
ENDIF;

IF (Activity @<> 'Reject_Data' & Management_Organization @<> 'NL_Reject' 
& Legal_Organization @<> 'NL_TM1_Reject' & Indicator @<> 'Reject_Data'  );
   nb_load=nb_load+1;
ENDIF;




