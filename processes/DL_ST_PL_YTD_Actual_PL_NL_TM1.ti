ELSE;
    Activity = 'Reject_Data';
ENDIF;


#-- Partner
IF( vPartner @= '' );
    Partner = 'SU9999';
ELSE;
    IF( Dimix( 'Partner' , vPartner ) = 0 );
          Partner='Other_VE_entities';
    ELSE;
          Partner = vPartner;
    ENDIF;
ENDIF;

#-- Management_Organization
IF (Management_Organization @<> 'NL_Reject' & Legal_Organization @<> 'NL_TM1_Reject');
   Management_Organization = CellGetS( 'TC_CONTRACT' , vManagement_Unit , vEntity , 'STR_VAR1' );
ENDIF;

#-- Indicator
IF (Indicator @<>'Reject_Data');
   Indicator = CellGetS( zCube_Ind , vIndicator , 'STR_VAR1' );
ENDIF;

################################################################################
